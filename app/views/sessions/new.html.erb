<h3>Taskleaf</h3>

<h2>ログイン</h2>
<%= form_with(scope: :session, url: sessions_path, local: true) do |form| %>

  <div><%= form.label "メールアドレス" %>
  <%= form.email_field :email %></div>

  <div><%= form.label "パスワード" %>
  <%= form.password_field :password %></div>

  <%= form.submit "ログインする" %>
<% end %>